{
    "name": "crhg/remo-client",
    "description": "Client for nature api",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://github.com/crhg/remo-client",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Manabu Matsui",
            "email": "manabu.matsui@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": ">=1.12"
    },
    "autoload": {
        "psr-4": { "Crhg\\RemoClient\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Crhg\\RemoClient\\" : "test/" }
    }
}
